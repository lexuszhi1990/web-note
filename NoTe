#git hub 
## register in github ---first of all
   create a user in github
## home 
     git config --global user.name "lexus_zhi"
     git config --global user.email lexuszhi1990@gmail.com
     mkdir lexus
     cd lexus
     git init
     touch README
     git add README
     git commit -m 'first commit'

                  
##  publickey
    ssh-keygen -t dsa
    sudo apt-get install xclip
    cat /home/jiesse/.ssh/id_dsa.pub| xclip -sel clip
    ssh git@github.com 
    ssh -v git@github.com
    
##  publickey where
    https://github.com/account
    account admin
    ssh public key
##  complish
    git remote add origin git@github.com:lexuszhi1990/lexus.git
    git push origin master              
